# Server Configuration
PORT=8080
ENV=development

# Database
DATABASE_URL=postgresql://postgres:postgres_dev_password@localhost:5432/housepoints_go?sslmode=disable

# Redis
REDIS_URL=redis://localhost:6379/0

# JWT
JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRY=24h

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# Logging
LOG_LEVEL=debug
LOG_FORMAT=json

# Multi-tenant
DEFAULT_FAMILY_PLAN=free
MAX_FAMILIES_PER_POD=500
